{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Orders_SQL",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "rank1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     Rank_ as integer",
				"}",
				"source(output(",
				"          Row_ID as string,",
				"          Order_Priority as string,",
				"          Discount as string,",
				"          Unit_Price as string,",
				"          Shipping_Cost as string,",
				"          Customer_ID as string,",
				"          Customer_Name as string,",
				"          Ship_Mode as string,",
				"          Customer_Segment as string,",
				"          Product_Category as string,",
				"          Product_Sub_Category as string,",
				"          Product_Container as string,",
				"          Product_Name as string,",
				"          Product_Base_Margin as string,",
				"          Country as string,",
				"          Region as string,",
				"          State_or_Province as string,",
				"          City as string,",
				"          Postal_Code as string,",
				"          Order_Date as string,",
				"          Ship_Date as string,",
				"          Profit as integer,",
				"          Quantity_Ordered_New as string,",
				"          Sales as string,",
				"          Order_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 rank(asc(Profit, true),",
				"     output(Rank as long)) ~> rank1"
			]
		}
	}
}